            <Button fx:id="option1" layoutX="462.0" layoutY="88.0" mnemonicParsing="false" text="Button" />
            <Button fx:id="option2" layoutX="462.0" layoutY="137.0" mnemonicParsing="false" text="Button" />
            <Button fx:id="option3" layoutX="462.0" layoutY="188.0" mnemonicParsing="false" text="Button" />
            <Button fx:id="option4" layoutX="462.0" layoutY="237.0" mnemonicParsing="false" text="Button" />
         </children>
      </Pane>
      <Pane fx:id="welcomePane" layoutX="10.0" layoutY="10.0" prefHeight="340.0" prefWidth="600.0">
         <children>
            <Button layoutX="275.0" layoutY="195.0" mnemonicParsing="false" onAction="#handleStart" text="Start" />
