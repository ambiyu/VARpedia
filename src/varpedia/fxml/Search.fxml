<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="varpedia.controllers.SearchController">
   <children>
      <Text fx:id="searchingText" layoutX="375.0" layoutY="248.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Searching..." textAlignment="CENTER" visible="false">
         <font>
            <Font size="18.0" />
         </font></Text>
      <TextField fx:id="textField" layoutX="273.0" layoutY="261.0" onAction="#searchWiki" promptText="Enter your search term">
         <font>
            <Font size="20.0" />
         </font></TextField>
      <Button fx:id="searchBtn" layoutX="535.0" layoutY="262.0" mnemonicParsing="false" onAction="#searchWiki" text="Search">
         <font>
            <Font size="19.0" />
         </font></Button>
      <Button fx:id="backBtn" layoutX="358.0" layoutY="547.0" mnemonicParsing="false" onAction="#back" text="Return to Menu">
         <font>
            <Font size="20.0" />
         </font></Button>
      <Label layoutX="274.0" layoutY="31.0" text="Wikipedia Search">
         <font>
            <Font name="System Bold" size="36.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
