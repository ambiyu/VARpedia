<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.media.MediaView?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane id="pane" prefHeight="600.0" prefWidth="900.0" stylesheets="@stylesheet.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
    <Button id="yellowBtn" layoutX="14.0" layoutY="545.0" mnemonicParsing="false" onAction="#handleBack" prefHeight="39.0" prefWidth="100.0" text="Back">
         <font>
            <Font size="20.0" />
         </font></Button>
      <Button id="stdBtn" fx:id="playPauseBtn" layoutX="400.0" layoutY="545.0" mnemonicParsing="false" onAction="#playPause" prefHeight="39.0" prefWidth="100.0" text="Pause">
         <font>
            <Font size="20.0" />
         </font></Button>
      <Button id="stdBtn" layoutX="722.0" layoutY="545.0" mnemonicParsing="false" onAction="#handleMute" text="Mute">
         <font>
            <Font size="20.0" />
         </font></Button>
      <Button id="stdBtn" layoutX="515.0" layoutY="545.0" mnemonicParsing="false" onAction="#handleForward" prefHeight="39.0" prefWidth="65.0" text="&gt;&gt;">
         <font>
            <Font size="20.0" />
         </font></Button>
      <Button id="stdBtn" layoutX="319.0" layoutY="545.0" mnemonicParsing="false" onAction="#handleBackward" prefHeight="39.0" prefWidth="65.0" text="&lt;&lt;">
         <font>
            <Font size="20.0" />
         </font></Button>
      <Text fx:id="currentTime" layoutX="430.0" layoutY="536.0" strokeType="OUTSIDE" strokeWidth="0.0" text="0:00">
         <font>
            <Font size="18.0" />
         </font></Text>
      <BorderPane fx:id="borderPane" prefHeight="500.0" prefWidth="900.0">
         <top>
            <MediaView fx:id="mediaView" fitHeight="500.0" fitWidth="900.0" BorderPane.alignment="CENTER" />
         </top>
      </BorderPane>
   </children>
</AnchorPane>
